void sort012(int a[], int n)
    {
        int zero=0,one=0,two=n-1;
        while(one<=two){
            if(a[one]==0){
                swap(a[zero],a[one]);
                zero++;
                one++;
            }
            else if(a[one]==2){
                swap(a[one],a[two]);
                two--;
            }
            else
                one++;
            
        }
        // code here 
    }
